{
  "name": "@danteissaias/dash",
  "version": "0.0.4",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    }
  },
  "scripts": {
    "dev": "cd dev && tsx ../dev.ts",
    "build": "tsup && vite build --outDir ./dist/app",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@danteissaias/ds": "^0.0.33",
    "@tanstack/react-table": "^8.7.9",
    "app-root-path": "^3.1.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "vite": "^4.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5",
    "vite-plugin-top-level-await": "^1.3.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
}
